#include <stdio.h>
int main()
{
    int a[] = {5, 8, 3, 9, 6, 2, 10, 7, -1, 4};
    int min, max, i;
    min = a[0];
    max = a[0];
    for (i = 1; i < 10; i++)
    {
        if (a[i] < min)
            min = a[i];
        else if (a[i] > max)
            max = a[i];
    }
    printf("%d %d\n", min, max);
    return 0;
}
