#include <stdio.h>
int main()
{
    int a[] = {5, 8, 3, 9, 6, 2, 10, 7, -1, 4};
    int i, j;
    int n = 10;
    int k;
    scanf("%d", &k);
    for (i = 0; i < n - 1; i++)
    {
        for (j = i + 1; j < n; j++)
        {
            if (a[i] + a[j] == k)
            {
                printf("%d+%d=%d\n", a[i], a[j], k);
            }
        }
    }
    return 0;
}
